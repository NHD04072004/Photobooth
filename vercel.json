{
  "version": 2,
  "builds": [
    {
      "src": "./main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "excludeFiles": "{.next,*.cache,node_modules,public,__pycache__}/**"
    }
  }
}